<%- include partials/header.ejs %>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label class="form-label">Title</label>
    <input type="text" name="postTitle" class="form-control" required autocomplete="off" autofocus>
    <label class="form-label">Post</label>
    <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>




